.main{
  bottom: 0;
  font-size: 30rpx;
}
.cart-empty-wrap {
  display: none;
  text-align: center;
  padding-top: 20%;
  font-size: 28rpx;
  color: #999;
  position: relative;
}
.cart-dingwei{
  width: 180rpx;
  height: 180rpx;
  border-radius: 80rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
}
.cart-dingwei image{
  width: 100%;
  height: 100%;
  border-radius: 80rpx;
}
.cart-empty-wrap text{
  margin: 24rpx 0;
}
.guangguang{
  border: 1px solid lightgray;
  border-radius: 6rpx;
  padding: 8rpx 60rpx;
  position: absolute;
  bottom: -110rpx;
  left: 267rpx;
}

.user-news{
  width: 100%;
  font-size: 28rpx;
  background-image: url('data:image/png;base64,'),url('data:image/png;base64,');
  background-position: left top,left bottom;
  background-size: auto 6rpx;
  background-color: #ffffff;
  background-repeat: repeat-x;
  margin-bottom: 20rpx;
  padding: 18rpx 0;
}
.user-news .tr {
  padding: 10rpx 0;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.user-news .th{
  padding: 14rpx 28rpx 14rpx 20rpx;
  text-align: left;
}
.user-news .td{
  padding: 14rpx 0;
}
.user-news .amend{
  display: inline-block;
  width: 17%;
  background: url('data:image/png;base64,') right center no-repeat;
  background-size: 40rpx;
  position: absolute;
  right: 0;
}
.text-hidden{
  visibility: hidden;
}

.cart-group-wrap{
  background-color: #fff;
  position: relative;
}
.cart-market-wrap{
  font-size: 28rpx;
  height: 70rpx;
  line-height: 60rpx;
  padding-left: 28rpx;
  color: #999999;
}
.cart-market-wrap::before{
  content: '';
  display: inline-block;
  width: 8rpx;
  height: 24rpx;
  background-color: #ffd600; 
  margin-right: 6rpx;
  border-radius: 4rpx;
  vertical-align: -4rpx;
}
.qisong{
  color:#999;
  font-size:24rpx;
  padding-left: 26rpx;
  height: 44rpx;
  line-height: 40rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;    
}
.cart-addBtn{
  position: absolute;
  right: 28rpx;
  top: 10rpx;
  font-size:24rpx;
  width: 140rpx;
  height: 48rpx;
  line-height: 48rpx;
  border-radius: 24rpx;
  border:1px solid #f40;
  color: #f40;
  text-align: center;
}
.picker{
  position: relative;
  border: 1px solid white;
  box-sizing: border-box;
  border-top-color: #e0e0e0;
  border-bottom-color: #e0e0e0;
  height: 100rpx;
  line-height: 96rpx;
  font-size: 28rpx;
  padding: 0 26rpx; 
  background: url('data:image/png;base64,') right center no-repeat;
  background-size: 40rpx;
  overflow: hidden;
}
.picker .yuding{
  float: right;
  padding-right: 28rpx;
}
.picker .receive{
  color: #f40;
}
.cart-receive-remark{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  padding-left: 26rpx;
  padding-right: 36rpx;
}
.remark-input{
  display:inline-block;  
  height: 64rpx;
  line-height: 64rpx;
  outline: none;
  font-size: 28rpx;
  padding-left: 20rpx;
  border: 1px solid #e0e0e0;
  border-radius: 12rpx;
  width: 75%; 
  position: relative;
  top: 18rpx;
}
/*商品展示区  */
.group-item-wrap{
  width:100%;
  background-color: #fff;
}
.group-item{
  display: inline-block;
  width: 100%;
  border-bottom: 1px solid lightgray;
  border-top: 1px solid lightgray;
  position: relative;
}
.group-item-checkbox{
  display: inline-block;
  background: url("data:image/png;base64,") center center no-repeat;
  -webkit-background-size: 36rpx 36rpx;
  background-size: 36rpx 36rpx;
  width: 100rpx;
  position: relative;
  top: -70rpx;
}
.group-item-checkbox.active{
  background-image: url("data:image/png;base64,");
}
.group-item-img{
  display: inline-block;
  width: 180rpx;
  height: 96rpx;
}
.group-item-img image{
  width: 100%;
  height: 192rpx;
}
.products-list-div{
  margin-bottom: 40rpx;
  padding-top: 20rpx;
  width: 60%;
  float: right;
  margin-left: 8rpx;
  color:#6d6d6d;
}
.products-list-title{
  margin-bottom: 70rpx;
}
.products-list-title .p{
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-size: 28rpx;
}
.products-list-content{
  color:#6d6d6d;
}

.btn-wrap{
  position: absolute;
  right: 0;
  bottom: 0;
  width: 180rpx;
  height:100rpx;
  line-height: 120rpx;
}
.btn-wrap text:nth-child(2n+1){
  display: inline-block;
    width: 50rpx;
    height: 50rpx;
    line-height: 44rpx;
    text-align: center;
    border: 1px solid lightgray;
    color: #f40;
    border-radius: 50%;
    font-size: 48rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.btn-wrap text:nth-child(2n+1).active{
  border-color: #f40;
}
.btn-wrap text:nth-child(2){
  display: inline-block;
    width: 40rpx;
    height: 60rpx;
    line-height: 50rpx;
    text-align: center;
    position: relative;
    top: 8rpx;
    font-size: 28rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.total-wrap{
  width: 100%;
  background-color: #fff;
  height: 100rpx;
  line-height: 100rpx; 
  font-size: 28rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.all-total{
  display: inline-block;
  min-width: 100rpx;
  padding-left: 50rpx;
  margin-left: 28rpx;
  background-position: left center;
  background-repeat: no-repeat;
  background-size: 36rpx 36rpx;
}
.all-total-false{
  background-image: url('data:image/png;base64,')
}
.all-total-true{
  background-image: url('data:image/png;base64,')
}
.total-price{
  color: #f40;
}
.total-btn{
  display: inline-block;
  background-color: #ffd600;
  width: 200rpx;
  text-align: center;
  position: absolute;
  right: 0;
}